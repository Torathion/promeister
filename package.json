{
  "name": "promeister",
  "version": "1.0.1",
  "description": "Extended promises with a lot of vital features. The AIO Promise class.",
  "types": "./index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "node esbuild.config.js",
    "lint": "eslint src --fix",
    "format": "npx prettier --write */**.ts",
    "test": "vitest"
  },
  "keywords": [
    "promise",
    "abort",
    "interrupt",
    "timeout",
    "data structure",
    "utility"
  ],
  "author": "Torathion",
  "homepage": "https://github.com/Torathion/promeister",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Torathion/promeister.git"
  },
  "bugs": {
    "url": "https://github.com/Torathion/promeister/issues"
  },
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "packageManager": "pnpm@10.8.1",
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@vitest/coverage-v8": "^3.1.2",
    "esbuild": "^0.25.2",
    "eslint": "^9.25.1",
    "eslint-config-shiny": "^4.1.0",
    "jiti": "^2.4.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "bitbob": "^3.0.1",
    "typestar": "^1.9.1"
  }
}
